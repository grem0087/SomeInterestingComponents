    "lint-css": "stylelint ./src/**/*.css",
    "lint-js": "eslint ./src/**/*.js --ext .js,.jsx",
    "lint": "npm run lint-css && npm run lint-js",
    "karma": "karma start",
    "test": "npm run lint && npm run karma",
    "test:watch": "node config/tests/build.js watch",
    "precommit": "lint-staged"
  },
